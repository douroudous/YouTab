<h2>Artists</h2>
<h3><%= link_to "New Artist", new_artist_path %></h3>
<p class="small-6 columns">Title</p>
<p class="small-6 columns"># of Tabs</p>
<% @artists.each do |artist| %>
  <p class="small-6 columns"><%= link_to "#{artist.name}", artist %> </p>
  <p class="small-6 columns"><%= artist.songs.length %></p>
<% end %>
