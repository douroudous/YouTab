<h2>Edit Tab Form</h2>

<%= form_for(@song) do |f| %>
  <div class="inline-block new-song">
    <div class="text">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>

    <div class="text">
      <%= f.label :artist %>
      <%= f.text_field :artist %>
    </div>

      <%= f.submit "Update Tab"%>

</div>
<% end %>
